.paging{
	height: 60px;
	font-size: 12px;
	font-family: "微软雅黑";
	overflow: hidden;
}
.paging .left{
	float: left;
	line-height: 60px;
}
.paging .goPage{
	float: right;
	margin-top: 16px;
	line-height: 22px;
}
.paging .goPage input{
	width: 36px;
	border:1px solid #e0e0e0;
	text-align: center;
	
    height: 22px;
    padding: 2px 5px;
    vertical-align: top;
    outline: none;
    -moz-border-radius: 3px;
    -webkit-border-radius: 3px;
    border-radius: 3px;
	
}
.paging .right{
	float: right;
}
.myPage {
	padding: 15px 10px 20px;
	color: #ccc;
	text-align: center;
	font-family: "微软雅黑";
}

.myPage a {
	display: inline-block;
	height: 25px;
	margin: 0 2px;
	padding: 0 10px;
	color: #555;
	line-height: 25px;
	border: 1px solid #e0e0e0;
	border-radius: 4px;
	vertical-align: middle;
	text-decoration: none;
}
.goPage a{
	color: #555;
	text-decoration: none;
}
.myPage a:hover {
	text-decoration: none;
	border: 1px solid #428bca;
}

.myPage span.current {
	display: inline-block;
	margin: 0 2px;
	padding: 0 10px;
	height: 25px;
	line-height: 25px;
	color: #fff;
	background-color: #428bca;
	border: 1px solid #428bca;
	border-radius: 4px;
	vertical-align: middle;
}

.myPage span.disabled {
	display: inline-block;
	height: 25px;
	line-height: 25px;
	padding: 0 10px;
	margin: 0 2px;
	color: #bfbfbf;
	background: #f2f2f2;
	border: 1px solid #bfbfbf;
	border-radius: 4px;
	vertical-align: middle;
	cursor:not-allowed;
}