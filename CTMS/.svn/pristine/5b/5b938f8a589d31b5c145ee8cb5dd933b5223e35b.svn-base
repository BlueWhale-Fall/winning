<script type="text/javascript">
    var activeTagInput;
    jQuery(document).ready(function () {
        leftInit(); //加载菜单
        App.init(); // initlayout and core plugins
        Index.init();
        $('input[name=tagsinput]').tagsinput('input');
    });
    $(function () {
        $('.add-right').popover({
            html: true,
            content: function () {
                return `<div>
        <input type="text" class="expiryDate-detail-input1 infoselects" value="" name="a"><i class="fa fa-search1"></i>
        <div class="editorbody1">
            <table class="table table-striped textalign-c tab1">
                <tbody>
                    <tr>
                        <td>
                            <input type="checkbox">
                        </td>
                        <td>1</td>
                    </tr>
                    <tr>
                        <td>
                            <input type="checkbox">
                        </td>
                        <td>2</td>
                        <tr>
                            <td>
                                <input type="checkbox">
                            </td>
                            <td>3</td>
                        </tr>
                        <tr>
                            <td>
                                <input type="checkbox">
                            </td>
                            <td>4</td>
                        </tr>
                        <tr>
                            <td>
                                <input type="checkbox">
                            </td>
                            <td>5</td>
                        </tr>
                        <tr>
                            <td>
                                <input type="checkbox">
                            </td>
                            <td>6</td>
                        </tr>
                </tbody>
            </table>
        </div>
        <div class="modal-footer">
            <button type="button" class="btn  btn-primary" id="save">保存</button>
            <button type="button" class="btn  btn-default" data-dismiss="modal" id="cancel">取消</button>
        </div>
    </div>`;
            },
            container: 'body',
            placement: 'top',

        }).on('show.bs.popover', function () {
            $('.popover').hide(); // 关闭其他的弹层 一次只显示一个
            activeTagInput = $(this).parent().find('input[name=tagsinput]');
        });
    });
    $(document.body).delegate('#cancel', 'click', function () {
        $('.popover').hide();
    });

    $(document.body).delegate('#save', 'click', function () {
        var child = $(this).parent('.modal-footer').prev('.editorbody1').find('tbody tr')
        child.each(function (index, element) {
            if ($(element).find('td:first input').attr("checked")) {
                var checkedValue = $(this).children('td:nth-child(2)').text();

                activeTagInput.tagsinput('add', checkedValue);
            }
        });
        $('.popover').hide();
    });
    $(document.body).delegate('.clear', 'click', function () {
        console.log('Clicked clear');
        $('.popover').hide();
    })
</script>
