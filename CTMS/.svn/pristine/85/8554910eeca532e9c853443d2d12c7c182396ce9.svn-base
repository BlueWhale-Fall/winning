/*遮罩*/

.tip-content {
    position: fixed;
    top: 50%;
    left: 50%;
    z-index: 20001;
    width: 200px;
    height: 100px;
    margin-left: -100px;
    margin-top: -50px;
    border: 1px solid red;
    -webkit-border-radius: 2px;
    -moz-border-radius: 2px;
    border-radius: 2px;
    background: #fff;
    border: 1px solid #E0E0E0;
    box-shadow: 0px 2px 3px 0px rgba(0, 0, 0, 0.10);
    display: none;
}

.tip-mask {
    position: fixed;
    width: 100%;
    height: 100%;
    left: 0;
    top: 0;
    z-index: 2000;
    background: #000;
    opacity: 0.4;
    filter: alpha(opacity=40);
    display: none;
}

.tip-word {
    margin: 15px auto;
    color: #333333;
    font-size: 12px;
}

.tip-word>img {
    float: left;
    width: 18px;
    height: 18px;
    margin-left: 20px;
    margin-right: 10px;
}

.tip-content a {
    padding: 3px 18px;
    color: #FCFCFC;
    font-size: 12px;
    font-family: "微软雅黑";
    text-align: center;
    text-decoration: none;
    border-radius: 2px;
}

.tip-btn {
    padding-top: 15px;
    text-align: center;
}

.tip-content a:hover {
    text-decoration: none;
}

.tip-content .cancel {
    color: #666;
    background: #fff;
    border: 1px solid #E0E0E0;
}

.tip-content .ok {
    margin-left: 20px;
    color: #FCFCFC;
    border: 1px solid #3395FF;
    background: #3395FF;
}
