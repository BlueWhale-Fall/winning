/*步骤进度*/

.alt-msg {
    position: absolute;
    top: 35px;
    right: 10px;
    z-index: 1000;
    width: 570px;
    height: 80px;
    background: #FFFFFF;
    border: 1px solid #E0E0E0;
    box-shadow: 0px 2px 3px rgba(0, 0, 0, 0.10);
    -moz-border-radius: 3px;
    -webkit-border-radius: 3px;
    border-radius: 3px;
    display: none;
}

.alt-msg>ul {
    position: relative;
    margin-top: 32px;
    margin-left: 28px;
}

.alt-msg>ul>li {
    float: left;
}

.alt-msg>ul>li.active {
    float: left;
    background-color: #48A3FF;
}

.alt-msg .line {
    width: 90px;
    height: 2px;
    margin-top: 4px;
    background-color: #E0E0E0;
}

.alt-msg .loop {
    position: relative;
    width: 10px;
    height: 10px;
    -moz-border-radius: 50%;
    -webkit-border-radius: 50%;
    border-radius: 50%;
    background-color: #E0E0E0;
}

.alt-msg .loop>* {
    position: absolute;
    color: #333;
    font-size: 14px;
}

.alt-msg .loop>span {
    top: -23px;
    left: -16px;
    min-width: 40px;
}

.alt-msg .loop>i {
    top: 11px;
    left: -20px;
    font-size: 12px;
    font-style: normal;
}
