                },
                container: 'body',
                placement: 'top',

            }).on('show.bs.popover', function () {
                $('.popover').hide(); // 关闭其他的弹层 一次只显示一个
                activeTagInput = $(this).parent().find('input[name=tagsinput]');
            });
        });
        $(document.body).delegate('#cancel', 'click', function () {
            $('.popover').hide();
        });

        $(document.body).delegate('#save', 'click', function () {
            var child = $(this).parent('.modal-footer').prev('.editorbody1').find('tbody tr')
            child.each(function (index, element) {
                if ($(element).find('td:first input').attr("checked")) {
                    var checkedValue = $(this).children('td:nth-child(2)').text();

                    activeTagInput.tagsinput('add', checkedValue);
                }
            });
            $('.popover').hide();
        });
