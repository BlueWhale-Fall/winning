            <!-- 页面开始 -->
            <div class="page-content project">
            <!-- BEGIN PAGE HEADER-->
                <div class="contenthead">
                    <div class="contenthead-title">表单验证</div>
                </div>
            <!-- END PAGE HEADER-->
                <!-- BEGIN DASHBOARD STATS -->
                <div class="date-main">
                    <div class="margin-right-20 margin-top-10 ">
                        <form id="signupForm" method="get" action="" autocomplete="off">
                            <div class="margin-bottom-10 clearfix">
                                <div class="col-md-2 text-right">必填项</div>
                                <div class="col-md-4">
                                    <input class="form-control infoselect" name="required">
                                </div>
                                <div class="col-md-2 text-right">电子邮件项(email)</div>
                                <div class="col-md-4">
                                    <input class="form-control infoselect" name="email">
                                </div>
                            </div>
                            <div class="margin-bottom-10 clearfix">
                                <div class="col-md-2 text-right">网址项(url)</div>
                                <div class="col-md-4">
                                    <input class="form-control infoselect" name="url">
                                </div>
                                <div class="col-md-2 text-right">合法的信用卡号</div>
                                <div class="col-md-4">
                                    <input class="form-control infoselect" name="creditcard">
                                </div>
                            </div>
                            <div class="margin-bottom-10 clearfix">
                                <div class="col-md-2 text-right">数字项</div>
                                <div class="col-md-4">
                                    <input class="form-control infoselect" name="number">
                                </div>
                                <div class="col-md-2 text-right">整数项</div>
                                <div class="col-md-4">
                                    <input class="form-control infoselect" name="digits">
                                </div>
                            </div>
                            <div class="margin-bottom-10 clearfix">
                                <div class="col-md-2 text-right">长度为6到8的密码项</div>
                                <div class="col-md-4">
                                    <input class="form-control infoselect" type="password" name="psd" id="psd">
                                </div>
                                <div class="col-md-2 text-right">确认密码项</div>
                                <div class="col-md-4">
                                    <input class="form-control infoselect"  type="password" name="equalTo">
                                </div>
                            </div>
                            <div class="margin-bottom-10 clearfix">
                                <div class="col-md-2 text-right">输入长度最多为5的字符串</div>
                                <div class="col-md-4">
                                    <input class="form-control infoselect" name="maxlength">
                                </div>
                                <div class="col-md-2 text-right">输入长度最少为10的字符串</div>
                                <div class="col-md-4">
                                    <input class="form-control infoselect" name="minlength">
                                </div>
                            </div>
                            <div class="margin-bottom-10 clearfix">
                                <div class="col-md-2 text-right">输入长度介于5和10之间的的字符串</div>
                                <div class="col-md-4">
                                    <input class="form-control infoselect" name="rangelength">
                                </div>
                                <div class="col-md-2 text-right">输入值介于5和10之间</div>
                                <div class="col-md-4">
                                    <input class="form-control infoselect" name="range">
                                </div>
                            </div>
                            <div class="margin-bottom-10 clearfix">
                                <div class="col-md-2 text-right">输入值不大于5</div>
                                <div class="col-md-4">
                                    <input class="form-control infoselect" name="max">
                                </div>
                                <div class="col-md-2 text-right">输入值不小于10</div>
                                <div class="col-md-4">
                                    <input class="form-control infoselect" name="min">
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-offset-1 col-md-9">
                                    <button class="btn btn-primary">提交</button>
                                </div>
                            </div>
                        </form>
