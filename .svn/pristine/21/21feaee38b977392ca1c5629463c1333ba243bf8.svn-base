    <script type="text/javascript">
        jQuery(document).ready(function () {
            /*加载菜单设置当前菜单*/
            leftInit(); 
            var _current=$("#sub1",$("#leftmenu"));
            _current.addClass("open").find("ul").css("display","block");
            _current.find("span.arrow").addClass("open");
            $("a[href='组织管理.html']",$("#leftmenu")).addClass("menuCur");
            App.init(); // initlayout and core plugins
            Index.init();
            /*表单验证*/
            $("#signupForm").validate({
                rules: {
                    required: {
                        required: true, 
                        minlength:5
                    },
                    email: {
                        email: true
                    },
                    url :{
                        url:true
                    },
                    number :{
                        number:true
                    },
                    digits :{
                        digits:true
                    },
                    creditcard :{
                        creditcard:true
                    },
                    psd :{
                        required: true,
                        rangelength:[6,8]
                    },
                    equalTo :{
                        required: true,
                        equalTo:"#psd"
                    },
                    maxlength :{
                        maxlength:5
                    },
                    minlength :{
                        minlength:10
                    },
                    rangelength :{
                        rangelength:[5,10]
                    },
                    max :{
                        max:5
                    },
                    min :{
                        min:10
                    },
                    range :{
                        range:[5,10]
                    }
                },
                messages:  {
                    required: {
                        required: "这是必填字段"
                    },
                    email: {
                        email: "请输入有效的电子邮件地址"
                    },
                    url :{
                        url: "请输入有效的网址"
                    },
                    number :{
                        number: "请输入有效的数字"
                    },
                    digits :{
                        digits: "只能输入整数"
                    },
                    creditcard :{
                        creditcard: "请输入有效的信用卡号码"
                    },
                    psd :{
                        required: "这是必填字段",
                        rangelength:"密码长度在6至8位之间"
                    },
                    equalTo :{
                        required: "这是必填字段",
                        equalTo: "你的输入不相同"
                    },
                    maxlength :{
                        maxlength:"最多可输入五个字符"
                    },
                    minlength :{
                        minlength:"最少输入10个字符"
                    },
                    rangelength :{
                        rangelength:"请输入长度在5到10之间的字符串"
                    },
                    max :{
                        max:"请输入不大于5的数值"
                    },
                    min :{
                        min:"请输入不小于10的数值"
                    },
                    range :{
                        range:"请输入长度在5到10之间的数值"
                    }
                }
            });
        });
    </script>
