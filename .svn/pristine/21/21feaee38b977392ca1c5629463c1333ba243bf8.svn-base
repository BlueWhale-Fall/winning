             <!-- 左侧菜单开始 -->
             <div id="medicinemenu" class="page-sidebar navbar-collapse collapse">

            </div>
            <!-- 左侧菜单结束 -->
            <!--右侧菜单开始-->
            <div class="offsidebar closed ie" id="offsidebar">

            </div>
