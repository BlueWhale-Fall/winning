.mainbody .left-ani span {
    display: inline-block;
    position: absolute;
}

.mainbody .ducts {
    width: 318px;
    height: 85px;
    position: absolute;
    left: 198px;
    top: 10px;
}

.mainbody .ducts span {
    display: inline-block;
    background-color: #FFE19F;
}

.ducts .duct1 {
    width: 7px;
    height: 0px;
    left: 1px;
    bottom: 0px;
    opacity: 0;
    -webkit-animation: duct1 1600ms infinite linear;
    -moz-animation: duct1 1600ms infinite linear;
    animation: duct1 1600ms infinite linear;
}

.ducts .duct3,
.ducts .duct4 {
    width: 7px;
    opacity: 0;
}

.ducts .duct2 {
    height: 7px;
    top: 2px;
    -webkit-animation: duct2 3000ms infinite linear;
    -moz-animation: duct2 3000ms infinite linear;
    animation: duct2 3000ms infinite linear;
    -webkit-animation-delay: 900ms;
    -moz-animation-delay: 900ms;
    animation-delay: 900ms;
}

.ducts .duct3 {
    left: 155px;
    animation: duct34 3000ms infinite linear;
    animation-delay: 1700ms;
}

.ducts .duct4 {
    left: 312px;
    -webkit-animation: duct34 2950ms infinite linear;
    -moz-animation: duct34 2950ms infinite linear;
    animation: duct34 2950ms infinite linear;
    -webkit-animation-delay: 2400ms;
    -moz-animation-delay: 2400ms;
    animation-delay: 2400ms;
}


/*ducts_animation*/

@-webkit-keyframes duct1 {
    0% {
        height: 0px;
        bottom: 0;
        opacity: 0;
    }
    50% {
        height: 76px;
        bottom: 0;
        opacity: 1;
    }
    80% {
        opacity: 1;
    }
    100% {
        height: 0;
        top: 2px;
        opacity: 0;
    }
}
@-moz-keyframes duct1 {
    0% {
        height: 0px;
        bottom: 0;
        opacity: 0;
    }
    50% {
        height: 76px;
        bottom: 0;
        opacity: 1;
    }
    80% {
        opacity: 1;
    }
    100% {
        height: 0;
        top: 2px;
        opacity: 0;
    }
}
@keyframes duct1 {
    0% {
        height: 0px;
        bottom: 0;
        opacity: 0;
    }
    50% {
        height: 76px;
        bottom: 0;
        opacity: 1;
    }
    80% {
        opacity: 1;
    }
    100% {
        height: 0;
        top: 2px;
        opacity: 0;
    }
}

@-webkit-keyframes duct2 {
    0% {
        width: 0px;
        left: 1px;
        opacity: 1;
    }
    50% {
        width: 317px;
        opacity: 1;
    }
    80% {
        opacity: 1;
    }
    100% {
        width: 0;
        right: -1px;
        opacity: 0;
    }
}
@-moz-keyframes duct2 {
    0% {
        width: 0px;
        left: 1px;
        opacity: 1;
    }
    50% {
        width: 317px;
        opacity: 1;
    }
    80% {
        opacity: 1;
    }
    100% {
        width: 0;
        right: -1px;
        opacity: 0;
    }
}
@keyframes duct2 {
    0% {
        width: 0px;
        left: 1px;
        opacity: 1;
    }
    50% {
        width: 317px;
        opacity: 1;
    }
    80% {
        opacity: 1;
    }
    100% {
        width: 0;
        right: -1px;
        opacity: 0;
    }
}

@-webkit-keyframes duct34 {
    0% {
        height: 0px;
        top: 7px;
        opacity: 0.9
    }
    50% {
        bottom: 0;
        height: 76px;
    }
    100% {
        height: 0px;
        bottom: 0px;
        opacity: 0;
    }
}
@-moz-keyframes duct34 {
    0% {
        height: 0px;
        top: 7px;
        opacity: 0.9
    }
    50% {
        bottom: 0;
        height: 76px;
    }
    100% {
        height: 0px;
        bottom: 0px;
        opacity: 0;
    }
}
@keyframes duct34 {
    0% {
        height: 0px;
        top: 7px;
        opacity: 0.9
    }
    50% {
        bottom: 0;
        height: 76px;
    }
    100% {
        height: 0px;
        bottom: 0px;
        opacity: 0;
    }
}

