/*弹窗组件
 * 辅助页面展示效果，部署无需引用
 * */

* {
	margin: 0px;
	padding: 0px;
}

body {
    position: relative;
    font-size: 12px;
    background-color: #f8f8f8;
}
.container {
	display: block;
	margin: 0 auto;
	width: 1180px;
    min-height: 1000px;
    padding-top: 100px;
	font-size:12px;
	background-color: #fff;
    box-shadow: 0 0 5px rgba(0, 0, 0, 0.2);
}

.container table {
    display: block;
    margin: 20px auto;
    table-layout: fixed;
    width: 500px;
}
.container div {
	display: inline-block;
	width: 60px;
	margin: 10px 10px 0;
	padding: 5px 10px;
	border: 1px solid #e0e0e0;
	border-radius: 4px;
	color: #555;
	cursor: pointer;
	font-family: "microsoft yahei";
	background: #fff none repeat scroll 0 0;
	text-align: center;
}