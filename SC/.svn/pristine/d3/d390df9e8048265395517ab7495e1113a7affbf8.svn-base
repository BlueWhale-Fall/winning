@charset "utf-8";
html,
body {
	width: 100%;
	height: 100%;
}

body {
	font-family: "Microsoft YaHei";
	font-size: 18px !important;
	direction: ltr;
	cursor: default;
	width: 1920px;
	height: 1080px;
	overflow: hidden;
	background: url("../img/bg.png");
}
/*通用样式*/

.width-24{
	width: 24px;
}
.width-30{
	width: 30px;
}
.margin-top-6{
	margin-top: 6px;
}
.margin-top-7{
	margin-top: 7px;
}
.margin-left-2{
	margin-left: 2px;
}

/*头部部分header*/

header {
	width: 100%;
	height: 80px;
	padding: 0 45px;
	position: relative;
	background: rgba(0,0,0, .11);
	box-sizing: border-box;
}

header h1 {
	float: left;
	color: #fff;
	height: 80px;
	position: relative;
	line-height: 80px;
	font-size: 32px;
	font-weight: normal;
	margin-right: 140px;
}

header>ul li {
	float: left;
	position: relative;
	margin-right: 80px;
}

header>ul li.active a {
	opacity: 1;
	border-bottom: 4px solid #fff;
}

header>ul li a:hover:before {
	left: 0;
	right: 0;
}

header>ul li a:before {
	content: '';
	position: absolute;
	left: 50%;
	right: 50%;
	bottom: 0;
	background: #fff;
	height: 4px;
	-webkit-transition-property: 'left, right';
	transition-property: 'left, right';
	-webkit-transition-duration: .3s;
	transition-duration: .3s;
	-webkit-transition-timing-function: ease-out;
	transition-timing-function: ease-out;
}

header>ul li a:hover,
header>ul li a:active,
header>ul li a:focus{
	color: #fff;
}

header>ul li a {
	color: #fff;
	width: 150px;
	height: 72px;
	opacity: .6;
	font-size: 22px;
	line-height: 80px;
	text-align: center;
	display: inline-block;
}

header time span {
	color: #fff;
	height: 80px;
	line-height: 80px;
	float: right;
	font-size: 26px;
	right: 246px;
	position: absolute;
}

/*切换按钮*/
.changePage a {
	color: #fff;
	width: 124px;
	height: 42px;
	opacity: .8;
	float: right;
	right: 100px;
	line-height: 42px;
	font-size: 18px;
	margin-top: 19px;
	text-align: center;
	border: 1px solid #fff;
	border-radius: 8px;
}

/*页面主体*/

#main {
	width: 100%;
	height: 992px !important;
	padding: 20px;
	overflow: hidden;
	background: #F1F2F7;
	box-sizing: border-box;
}

/*尾部footer*/

footer {
	width: 100%;
	height: 8px;
	background-color: #075DFF;
}


/*personnel页面*/
.ward {
	width: 100%;
	height: 936px;
	box-sizing: border-box;
	box-shadow: 0 2px 10px 0 rgba(8,92,255,0.20);
	background-color: #fff;
}

.wardtitle {
	width: 100%;
	height: 48px;
	line-height: 48px;
	color: #333;
	font-size: 24px;
	text-align: center;
	border-bottom: 1px solid #F0F0F0;
}

.warddetail {
	width: 100%;
	height: 936px;
	position: relative;
	padding: 30px 0px 0px 30px;
}

.hd {
	bottom: 30px;
	right: 130px;
	position: absolute;
}

.hd ul li {
	float: left;
	width: 64px;
	height: 30px;
	color: #848484;
	font-size: 18px;
	line-height: 27px;
	text-align: center;
	border-radius: 2px;
	margin-left: 10px;
	border: 1px solid #DCDCDC;
	transition: all .5s;
}

.hd ul li.on,
.hd ul li:hover {
	color: #fff;
	background-color: #085DFF;
}

.bd ul li>div {
	float: left;
	width: 340px;
	height: 385px;
	margin-right: 30px;
	margin-bottom: 30px;
	border-radius: 8px;
	background: #FFFFFF;
	padding-top: 10px;
	box-sizing: border-box;
	border: 1px solid #FFFFFF;
	box-shadow: 0 2px 10px 0 rgba(8,92,255,0.20);
}

.bd ul li>div h1 {
	color: #085DFF;
	font-size: 28px;
	text-align: center;
	margin-top: 38px;
	margin-bottom: 15px;
}

.bd ul li>div h2 {
	color: #999;
	width: 280px;
	height: auto;
	font-size: 18px;
	line-height: 32px;
	margin: 60px auto 0;
	text-align: center;
}

.sick {
	width: 251px;
	height: 35px;
	margin: 34px auto 30px;
	box-sizing: border-box;
}

.sick span {
	width: 125px;
	height: 35px;
	line-height: 35px;
	font-size: 18px;
	text-align: center;
	display: inline-block;
	box-sizing: border-box;
	border: 1px solid #58a0ff;
}

.sick span:first-child {
	color: #fff;
	background-color: #57a1ff;
}

.sick span:last-child{
	color: #58a0ff;
	margin-left: -5px;
	background-color: #fff;
}

.paramedic {
	color: #999;
	width: 245px;
	height: 25px;
	overflow: hidden;
	margin: 0 auto;
	font-size: 18px;
}

.paramedic .atwork {
	color: #666;
}

.paramedic .leave {
	color: #ff0000;
}

.paramedic div:first-child {
	float: left;
}

.paramedic div:last-child {
	float: right;
}

/*自动翻页*/
.autopage {
	position: absolute;
	bottom: 30px;
	right: 32px;
	font-size: 14px;
}

.autopage .finaltime {
	color: #085DFF;
	letter-spacing: 0px;
}

/*personnel_end*/


