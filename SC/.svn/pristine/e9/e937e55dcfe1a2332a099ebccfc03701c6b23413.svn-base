*/

/*24H + 血氧饱和度 + 特殊治疗*/
#allday .numbox ul li .num,
#bloodoxygen .numbox ul li .num,
#cure .numbox ul li .num {
    margin: 6px 7px 7px 8px;
}

/*测血压 + 测心率*/
#bloodpressure .numbox ul li .num,
#heartrate .numbox ul li .num {
    margin: 9px 7px 7px 4px;
}

/*基础护理*/
#pulse .numbox ul li .num {
    margin: 10px 4px 7px 4px;
}

/*三大监控 + 药物过敏 + 其他*/
#control .numbox ul li .num,
#allergy .numbox ul li .num,
#other .numbox ul li .num {
    margin: 6px 7px 7px 13px;
}

