/*header.css*/
.head {
    height: 72px;
    line-height: 72px;
    width: 100%;
    background: #085DFF;
    color: #fff;
    font-size: 48px;
    text-align: center;
    position: relative;
}

.head .headtime {
    color: #fff;
    font-size: 32px;
    position: absolute;
    left: 20px;
}

.head .nurse {
    color: #fff;
    font-size: 22px;
    position: absolute;
    right: 29px;
    top: 0;
}

.headbody {
    width: 100%;
    height: 95px;
    background: #fff;
    -moz-box-shadow: 0px 3px 3px #e0e0e0;
    -webkit-box-shadow: 0px 3px 3px #e0e0e0;
    box-shadow: 0px 3px 3px #e0e0e0;
    /* For IE 8 */
    -ms-filter: "progid:DXImageTransform.Microsoft.Shadow(Strength=4, Direction=135, Color='#000000')";
}

.headmain {
    position: absolute;
    left: 110px;
    height: 110px;
    margin: 0 auto;
    padding: 5px 0;
}

.headboxright {
    width: 305px;
    float: left;
}

.headbox {
    height: 85px;
    float: left;
    padding: 0px;
}

.headborder {
    -moz-border-right: 1px solid #DEDEDE;
    -webkit-border-right: 1px solid #DEDEDE;
    border-right: 1px solid #DEDEDE;
    padding-right: 10px;
}

.headtextbox {
    height: 43px;
    float: left;
    line-height: 45px;
    width: 300px;
}

.headtext {
    min-width: 72px;
    float: left;
    text-align: right;
}

.leftimg {
    background: url('../img/people.png') center center no-repeat;
    width: 70px;
    margin-right: 10px;
}

.middleimg {
    width: 70px;
    background: url(../img/hosin.png) center right no-repeat;
    margin-right: 10px;
}

.rightimg {
    width: 70px;
    background: url(../img/follow.png) center right no-repeat;
    margin-right: 10px;
}

.headline {
    height: 50px;
    line-height: 50px;
    float: left;
}

.headhighline {
    height: 85px;
    line-height: 85px;
    width: 100%;
    float: left;
}

.lefttext {
    font-size: 24px;
    color: #3D414A;
    float: left;
}

.righttextnum {
    float: left;
    font-size: 42px;
    color: #1463F0;
}

.lefttextnum {
    float: left;
    font-size: 42px;
    color: #1463F0;
}

.headnum {
    font-size: 28px;
    color: #1463F0;
    float: left;
    min-width: 40px;
}

.font-red {
    color: #FF2A21;
}
/*header.css end*/

/*aside*/
.databody {
    padding: 20px 20px 15px 20px;
    background: #F1F2F7;
    overflow: auto;
}

#fastBSDetails {
    width: 1880px;
    height: 846px;
}

#fastBSDetails h2 {
    font-size: 24px;
    color: #1463F0;
    height: 55px;
    line-height: 55px;
}

#fastBSDetails .title {
    height: 55px;
    line-height: 55px;
}

#fastBSDetails .title li {
    -moz-box-sizing: border-box;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    font-size: 22px;
    color: #333;
    float: left;
    width: 235px;
}

#fastBSSlide>li>span {
    width: 235px;
}

#fastBSDetails .infoList li span {
    display: inline-block;
    color: #666;
}

.infoList > li:nth-child(2n) {
    background-color: #F7F7F7;
}

#fastBSDetails .infoList li .slide {
    height: 55px!important;
    float: left;
    cursor: default;
}

#fastBSDetails .infoList {
    height: 55px;
    float: left;
}

#fastBSDetails .infoList:nth-child(2n) {
    background-color: #F7F7F7;
}

#fastBSDetails .infoList li .num {
    color: #166AFF;
}

#fastBSDetails .infoList .mark {
    width: 24px;
    height: 24px;
}

#fastBSDetails {
    position: relative;
}

#screen {
    width: 400px;
    height: 32px;
    right: 10px;
    bottom: -5px;
    text-align: right;
    position: absolute;
}

#fastBSDetails>a {
    width: 80px;
    height: 32px;
    color: #666;
    top: 11px;
    right: 40px;
    font-size: 20px;
    line-height: 31px;
    position: absolute;
    display: inline-block;
    background: #FAFAFA;
    border-radius: 70px;
    text-align: center;
    font-weight: normal;
    text-decoration:none;
    border: 1px solid #E1E1E1;
}

#fastBSDetails>a:hover {
    color: #518DFF;
    border-color: #518DFF;
}

#fastBSDetails>a:active {
    color: #F9F9F9;
    border-color: #6d6d6d;
    background-color: #6d6d6d;
}


/*footer*/

footer {
    color: #fff;
    width: 100%;
    height: 38px;
    float: left;
    text-align: right;
    line-height: 35px;
    padding-right: 20px;
    background: #1463F0;
    box-sizing: border-box;
}
