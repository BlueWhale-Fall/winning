/*footer*/

footer {
    width: 100%;
    height: 32px;
    line-height: 32px;
    float: left;
    background: #1463F0;
    color: #fff;
}

footer .attention .floatL {
    line-height: 32px;
}

footer .voice {
    display: block;
    width: 32px;
    height: 32px;
    float: left;
    margin-right: 10px;
    background: url(../img/voice.png) center center no-repeat;
}

footer .attention {
    padding: 0 20px;
}